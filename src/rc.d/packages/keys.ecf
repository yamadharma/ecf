;;;

;; searching and replace
(global-set-key "\M-s" 'search-forward)
(global-set-key "\M-r" 'search-backward)
(global-set-key "\M-S" 'tags-search)
(global-set-key [?\C-\M-%] 'query-replace-regexp)

;; goto line n
(global-set-key "\M-g" 'goto-line)

;; miscellaneous
(global-set-key "\M-M" 'manual-entry)




