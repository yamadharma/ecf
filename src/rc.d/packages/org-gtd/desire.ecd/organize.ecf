;;; A package for using GTD with org-mode
;;; https://github.com/Trevoke/org-gtd.el

;;; Areas of focus
(setq org-gtd-organize-hooks '(org-gtd-set-area-of-focus org-set-tags-command))

(general-define-key
 :keymaps 'org-gtd-clarify-map
 "C-c c" #'("Organize" . org-gtd-organize))

